
{{#each this}}
<div>
    <h1 class="country__name">{{ name }}</h1>
    <img src="{{flag}}" alt="{{name}}" class="country__flag" width="360">
    <p class="country__capital">Capital: {{ capital }}</p>
    <p class="country__population">Population: {{ population }}</p>
    <ul class="country-list__languages">Languages:
        {{#each languages}}
        <li class="country-item__language">{{ name }}</li>
        {{/each}}
    </ul>
</div>
{{/each}}


